<script>
$(document).ready(function(){

  $("#BTN_CLICK").click(function(){
  
   var a= 9;
   var b= 4;
   var c=a%b;
   var d=a/b;  
   var e= Math.floor(d); 
   var f=0;
   if(c!=0){
    f=1;
   }else{
   f=0;
   }   
   var g=e+f;
   alert(g); 
    
  });
});
</script>
